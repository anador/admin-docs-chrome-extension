{
    "name": "Make admin panel great again",
    "description": "Ссылки на документацию по разделам в Confluence",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}